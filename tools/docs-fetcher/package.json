{
  "name": "@acme/docs-fetcher",
  "version": "0.0.1",
  "description": "Generic documentation fetcher for third-party libraries",
  "module": "./src/index.js",
  "type": "module",
  "main": "./src/index.js",
  "typings": "./src/index.d.ts",
  "scripts": {
    "start": "bun run src/cli.ts",
    "fetch": "bun run src/cli.ts",
    "fetch:all": "bun run src/cli.ts --all",
    "score": "bun run src/cli.ts score",
    "test": "vitest run -c vitest.config.ts",
    "test:watch": "vitest watch -c vitest.config.ts",
    "lint": "eslint . --config eslint.config.mjs"
  },
  "dependencies": {
    "fs-extra": "^11.1.1",
    "minimatch": "^9.0.3",
    "playwright": "^1.40.0",
    "tslib": "^2.3.0",
    "cheerio": "^1.0.0-rc.12",
    "node-fetch": "^3.3.2",
    "chalk": "^5.3.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.16.0",
    "@playwright/test": "^1.49.0",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^22.10.1",
    "@typescript-eslint/eslint-plugin": "^8.17.0",
    "@typescript-eslint/parser": "^8.17.0",
    "@vitest/coverage-v8": "^1.0.4",
    "bun-types": "^1.1.38",
    "eslint": "^9.16.0",
    "glob": "^11.0.0",
    "typescript-eslint": "^8.17.0",
    "vitest": "^1.0.4"
  }
}
