{
  "rules": [
    {
      "id": "@secretlint/secretlint-rule-preset-recommend"
    }
  ],
  "ignores": [
    ".git/**",
    "**/node_modules/**",
    "**/*.jpg",
    "**/*.png",
    "**/*.gif",
    "**/*.svg",
    "**/*.woff",
    "**/*.ttf",
    "**/*.eot",
    "**/*.mp4",
    "**/*.mp3",
    "**/*.ico",
    "**/dist/**",
    "**/build/**",
    "**/.next/**"
  ],
  "targets": ["**/*", ".*"]
}
